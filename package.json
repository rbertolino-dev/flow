{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:5174": "vite --port 5174",
    "prebuild": "if [ -f scripts/generate-version-file.sh ]; then chmod +x scripts/generate-version-file.sh && ./scripts/generate-version-file.sh || echo '⚠️ Script de versão não encontrado, continuando...'; else echo '⚠️ Script de versão não encontrado, criando versão básica...' && mkdir -p public && echo '{\"version\":\"0.0.0\",\"timestamp\":\"'$(date -u +%Y-%m-%dT%H:%M:%SZ)'\",\"changes\":\"Versão automática\"}' > public/version.json; fi",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-imports": "./scripts/validar-imports.sh src",
    "type-check": "tsc --noEmit",
    "validate": "./scripts/validar-codigo-completo.sh",
    "validate:quick": "./scripts/validar-codigo-completo.sh --skip-tests",
    "health-check": "./scripts/health-check-completo.sh",
    "health-check:fix": "./scripts/health-check-completo.sh --fix",
    "regression": "npm run lint && npm run build",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:e2e:auto": "./scripts/teste-automatico-completo.sh",
    "test:e2e:analyze": "./scripts/teste-automatico-completo.sh --report-only",
    "test:e2e:fix": "./scripts/teste-automatico-completo.sh auto --fix",
    "test:e2e:codegen": "playwright codegen http://localhost:8080",
    "test:e2e:human": "playwright test --grep @human-behavior",
    "test:e2e:accessibility": "playwright test --grep @accessibility",
    "test:e2e:performance": "playwright test --grep @performance",
    "test:e2e:visual": "playwright test --grep @visual",
    "test:new-feature": "./scripts/testar-nova-funcionalidade.sh",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "prepare": "husky install || true"
  },
